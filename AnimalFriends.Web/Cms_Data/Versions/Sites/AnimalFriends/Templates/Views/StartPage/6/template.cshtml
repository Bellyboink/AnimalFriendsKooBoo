
<div>
    <h3 class="title" @ViewHelper.Edit(ViewBag.StartPageBanner,"Image")>@Html.Raw(ViewBag.StartPageBanner.Image ?? "")</h3>
    <div class="content">   
        
        <div @ViewHelper.Edit(ViewBag.StartPageBanner,"Header")>
            @Html.Raw(ViewBag.StartPageBanner.Header ?? "")
        </div>

        <div @ViewHelper.Edit(ViewBag.StartPageBanner,"Text")>
            @Html.Raw(ViewBag.StartPageBanner.Text ?? "")
        </div>

    </div>
</div>