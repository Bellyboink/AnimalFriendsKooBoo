@if (Model != null)
{ 
<div>
	<h3 class="title" @ViewHelper.Edit(Model,"Identifier")>@Html.Raw(Model.Identifier ?? "")</h3>
	<div class="content">
		<div @ViewHelper.Edit(Model,"Description")>@Html.Raw(Model.Description ?? "")</div>
		<div @ViewHelper.Edit(Model,"Name")>@Html.Raw(Model.Name ?? "")</div>
	</div>
</div>
}